package HRlink1;
import java.util.*;
public class eight{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int a = 0, b = 1;
        for (int i = 0; i < N; i++) {
            System.out.print(a);
            if (i < N - 1) System.out.print(" ");
            int next = a + b;
            a = b;
            b = next;
        }
    }
}

